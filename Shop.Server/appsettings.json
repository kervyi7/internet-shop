{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    //"DefaultConnection": "Host={Host};Port={Port};Database={Database};Username={Username};Password={Password}"
  },
  "IdentityConfig": {
    "RequiredLength": 6,
    "RequireNonAlphanumeric": false,
    "RequireDigit": false,
    "RequireLowercase": false,
    "RequireUppercase": false,
    "AllowOnlyAlphanumericUserNames": false,
    "UserRequireUniqueEmail": false,
    "UserNameRegularExpression": "^[a-zA-Z0-9@_\\.]+$"
  },
  "AuthConfig": {
    "AccessTokenExpireTimeInMinutes": 600,
    "RefreshTokenExpireTimeInMinutes": 20160
  }
}
